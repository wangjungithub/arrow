[
    {
        "settings": [ "master" ],
        "name": "YahooLogin",
        "config": {
            "baseUrl": "http://www.yahoo.com"
        },
        "dataprovider" : {

            "Use Locator to Login" : {
                "group" : "func",
                "params" :{
                    "scenario": [
                        {
                            "page": "$$config.baseUrl$$"
                        },
                        {
                            "controller": "./sessiongroup.js",
                            "params": {
                                "page1": "http://sports.yahoo.com",
                                "page2": "http://www.flickr.com"
                            }
                        },
                        {
                            "test": "../test-title.js",
                            "title": "Yahoo!"
                        }
                    ]
                }
            }
        }
    },
    {
        "settings": [ "environment:development" ]
    }
]
